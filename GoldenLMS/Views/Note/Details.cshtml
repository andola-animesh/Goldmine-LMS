@model GoldenLMS.Models.NoteTbl

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
<div class="bg-white b-1px-gray-light b-top-none brad-bottom brad-tr b-bot-2px-gray-light">
    <div class="col-md-12">
        <h2>Note Details</h2>

        <fieldset>
            <legend></legend>

            <div class="row">
                <div class="col-md-2">
                    @Html.LabelFor(model => model.Attachment, new { @class = "control-label" })
                </div>
                <div class="col-md-4">
                    @Html.DisplayFor(model => model.Attachment, new { @class = "control-label" })
                </div>
            </div>

            <div class="row">
                <div class="col-md-12" style="height: 10px;">
                </div>
            </div>

            <div class="row">
                <div class="col-md-2">
                    @Html.LabelFor(model => model.Comment, new { @class = "control-label" })
                </div>
                <div class="col-md-4">
                    @Html.DisplayFor(model => model.Comment, new { @class = "control-label" })
                </div>
            </div>

            <div class="row">
                <div class="col-md-12" style="height: 10px;">
                </div>
            </div>
        </fieldset>
        <div class="row">
            <div class="col-md-10 pull-right">
                @Html.ActionLink("Edit", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("Back to List", "FileUpload")
            </div>
        </div>

    </div>
</div>
